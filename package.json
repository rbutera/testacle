{
  "name": "vscode-testacle",
  "displayName": "Testacle",
  "description": "Switch to source file from a test file, or vice versa, or open either in a new tab!",
  "version": "0.0.1",
  "publisher": "rbutera",
  "icon": "assets/icon.png",
  "engines": {
    "vscode": "^1.52.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:testacle.switch",
    "onCommand:testacle.open"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "testacle.open",
        "title": "Testacle: Split editor with Test/Implementation"
      },
      {
        "command": "testacle.switch",
        "title": "Testacle: Go to Test/Implementation"
      }
    ],
    "keybindings": [
      {
        "command": "testacle.open",
        "key": "ctrl+shift+t",
        "mac": "shift+cmd+t"
      },
      {
        "command": "testacle.switch",
        "key": "ctrl+alt+shift+t",
        "mac": "alt+shift+cmd+t"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "test": "jest out/test/unit/**.js",
    "unit:test": "jest out/test/unit/**.js --watch"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/node": "^12.11.7",
    "@types/ramda": "github:types/npm-ramda#dist",
    "@types/vscode": "^1.43.0",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "jest": "^26.6.3",
    "typescript": "^3.7.5",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "@types/jest": "^26.0.20",
    "ramda": "^0.27.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rbutera/vscode-testacle"
  },
  "bugs": {
    "url": "https://github.com/rbutera/vscode-testacle/issues"
  },
  "keywords": [
    "switch",
    "open",
    "tdd",
    "switcher",
    "navigate",
    "test",
    "spec"
  ]
}
